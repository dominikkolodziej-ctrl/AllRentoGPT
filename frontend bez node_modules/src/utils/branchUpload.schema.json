{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "additionalProperties": false,
    "required": ["Nazwa oddziału", "Miasto", "Kod pocztowy", "Adres", "Kod lokalizacji"],
    "properties": {
      "Nazwa oddziału": { "type": "string", "minLength": 1 },
      "Miasto": { "type": "string", "minLength": 1 },
      "Kod pocztowy": { "type": "string", "pattern": "^\\d{2}-\\d{3}$" },
      "Adres": { "type": "string", "minLength": 1 },
      "Kod lokalizacji": { "type": "string", "minLength": 1 }
    }
  }
}
